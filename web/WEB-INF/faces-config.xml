<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>selector</managed-bean-name>
        <managed-bean-class>Selector</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>util</managed-bean-name>
        <managed-bean-class>Util</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>customer</managed-bean-name>
        <managed-bean-class>Customer</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>user</managed-bean-name>
        <managed-bean-class>User</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>growbox</managed-bean-name>
        <managed-bean-class>GrowBox</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>garden</managed-bean-name>
        <managed-bean-class>Garden</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>seeds</managed-bean-name>
        <managed-bean-class>Seeds</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>crops</managed-bean-name>
        <managed-bean-class>Crops</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>marketplace</managed-bean-name>
        <managed-bean-class>Marketplace</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>test</managed-bean-name>
        <managed-bean-class>TestBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <!--    <managed-bean>
        <managed-bean-name>plantSpecies</managed-bean-name>
        <managed-bean-class>PlantSpecies</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>-->
    <managed-bean>
        <managed-bean-name>fruitingPlant</managed-bean-name>
        <managed-bean-class>FruitingPlant</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>rootPlant</managed-bean-name>
        <managed-bean-class>RootPlant</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>decorativePlant</managed-bean-name>
        <managed-bean-class>DecorativePlant</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>
            /index.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createAccount</from-outcome>
            <to-view-id>newUser.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>
            /newUser.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>createUser</from-outcome>
            <to-view-id>index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>
            /findCustomer.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>showCustomer</from-outcome>
            <to-view-id>displayCustomer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>
            /*
        </from-view-id>
        <navigation-case>
            <from-outcome>visitMarketplace</from-outcome>
            <to-view-id>marketplaceHome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>showSeedInventory</from-outcome>
            <to-view-id>showSeedInventory.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>showCropInventory</from-outcome>
            <to-view-id>showCropInventory.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ViewGarden</from-outcome>
            <to-view-id>home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Logout</from-outcome>
            <to-view-id>index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>


    <navigation-rule>
        <from-view-id>
            /showSeedInventory.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>insufficientSeeds.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>
            /showCropInventory.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>insufficientSeeds.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>
            /insufficientseeds.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>
            /insufficientCrops.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>
            /marketplaceHome.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>


